{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-treater": {
      "projectType": "library",
      "root": "projects/ng-treater",
      "sourceRoot": "projects/ng-treater/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/ng-treater/tsconfig.lib.json",
            "project": "projects/ng-treater/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-treater/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-treater/src/test.ts",
            "tsConfig": "projects/ng-treater/tsconfig.spec.json",
            "karmaConfig": "projects/ng-treater/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-treater/tsconfig.lib.json",
              "projects/ng-treater/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo": {
      "projectType": "library",
      "root": "projects/demo",
      "sourceRoot": "projects/demo/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/demo/tsconfig.lib.json",
            "project": "projects/demo/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/demo/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo/src/test.ts",
            "tsConfig": "projects/demo/tsconfig.spec.json",
            "karmaConfig": "projects/demo/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo/tsconfig.lib.json",
              "projects/demo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "cli": {
    "analytics": "9faf53cf-586d-490f-8ba6-1acb3e95d264"
  },
  "defaultProject": "ng-treater"
}